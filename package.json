{
  "name": "jkscript",
  "version": "3.1.1",
  "author": "Jordan Kowal",
  "license": "MIT",
  "homepage": "https://github.com/Jordan-Kowal/jkscript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jordan-Kowal/jkscript.git"
  },
  "bugs": {
    "url": "https://github.com/Jordan-Kowal/jkscript/issues"
  },
  "description": "Personal library with ES6 JavaScript utilities",
  "keywords": ["es6", "javascript", "js", "utilities"],
  "main": "dist/index.js",
  "files": ["dist/", "CHANGELOG.md", "LICENSE.md", "README.md"],
  "scripts": {
    "biome:format": "biome format ./src",
    "biome:lint": "biome lint ./src",
