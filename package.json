{
  "name": "jkscript",
  "version": "3.3.0",
  "author": "Jordan Kowal",
  "license": "MIT",
  "homepage": "https://github.com/Jordan-Kowal/jkscript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jordan-Kowal/jkscript.git"
  },
  "bugs": {
    "url": "https://github.com/Jordan-Kowal/jkscript/issues"
  },
  "description": "Personal library with ES6 JavaScript utilities",
  "keywords": [
    "es6",
    "typescript",
    "ts",
    "utilities"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "biome:format": "biome format ./src",
    "biome:lint": "biome lint ./src",
    "biome:check": "biome check ./src",
    "biome:check:apply": "biome check --apply ./src",
    "biome:check:apply-unsafe": "biome check --apply-unsafe ./src",
    "build": "tsc --declaration && babel --extensions .ts,.tsx,js,jsx src -d dist",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@babel/cli": "7.27.1",
    "@babel/core": "7.27.1",
    "@babel/preset-env": "7.27.1",
    "@babel/runtime": "7.27.1",
    "dayjs": "1.11.13"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.27.1",
    "@biomejs/biome": "1.9.4",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "^29.5.12",
    "circular-dependency-plugin": "5.2.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  }
}
